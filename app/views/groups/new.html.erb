<div class="register">
  <i class="fas fa-bars"></i>
  GROUPS
  <i class="fas fa-search"></i>
</div>
<br><br>
 <%=simple_form_for @group, url: user_groups_path(current_user), html: { method: :post } do |f|  %>
  <div>
    <%= f.text_field :name ,placeholder: "Name"%>
  </div>
  <div>
    <div class="input photo">upload an icon</div>
      <%= f.input :icon,:label => false %>
      <%= f.input :icon_cache, as: :hidden %>
  </div>
  <div>
  <div><br>
     <%= f.submit "Submit",class:"btn btn-success"%>
  </div>
<% end %>